    <bean name="sparkUtil" class="com.dmy.graduation.spark.SparkUtil"/>
    <bean name="scidOptimization" class="com.dmy.graduation.shuffle.SCIDOptimization"/>
    <bean name="defaultPartitionerMock" class="com.dmy.graduation.partitioner.mock.DefaultPartitionerMockImpl"/>
